{
    "proxmox_host": "{{env `PROXMOX_HOST`}}",
    "proxmox_insecure_tls": "false",
    "proxmox_node_name": "{{env `PROXMOX_NODE`}}",
    "proxmox_api_user": "{{env `PROXMOX_USER`}}@pve",
    "proxmox_api_password": "{{env `PROXMOX_PASSWORD`}}",
    "proxmox_pool": "Templates",
    "ssh_timeout": "60m",
    "vmid": "{{env `TEMPLATE_VMID`}}",
    "datastore": "{{env `PROXMOX_DATASTORE`}}",
    "datastore_type": "{{env `PROXMOX_DATASTORE_TYPE`}}",
    "start_on_boot": "true",
    "cloud_init": "true",
    "cloud_init_storage_pool": "{{env `PROXMOX_DATASTORE`}}",
    "iso_storage_pool": "{{env `PROXMOX_ISO_STORAGE`}}"
}